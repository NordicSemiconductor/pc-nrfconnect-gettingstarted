{
  "name": "pc-nrfconnect-gettingstarted",
  "version": "2.0.0-pre2",
  "displayName": "Getting Started Assistant",
  "description": "Guide for setting up toolchains and getting the code for nRF Connect SDK",
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/pc-nrfconnect-gettingstarted"
  },
  "author": "Nordic Semiconductor ASA",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "nrfconnect": "^3.8.0"
  },
  "main": "dist/bundle.js",
  "files": [
    "dist/",
    "icon.png",
    "LICENSE",
    "resources/data/"
  ],
  "scripts": {
    "dev": "nrfconnect-scripts build-watch",
    "webpack": "nrfconnect-scripts build-dev",
    "build": "nrfconnect-scripts build-prod",
    "lint-init": "nrfconnect-scripts lint-init",
    "lint": "nrfconnect-scripts lint index.jsx lib",
    "lintfix": "nrfconnect-scripts lint --fix index.jsx lib",
    "nordic-publish": "nrfconnect-scripts nordic-publish",
    "test": "nrfconnect-scripts test",
    "test-watch": "nrfconnect-scripts test --watch",
    "clean": "npm run clean-dist && npm run clean-modules",
    "clean-dist": "rimraf dist",
    "clean-modules": "rimraf \"node_modules/!(rimraf|.bin)\""
  },
  "devDependencies": {
    "linux-os-info": "^2.0.0",
    "pc-nrfconnect-shared": "github:NordicSemiconductor/pc-nrfconnect-shared#v5.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "^3.6.0",
    "react-redux": "7.2.5"
  },
  "dependencies": {
    "electron-store": "^2.0.0"
  },
  "bundledDependencies": [
    "electron-store"
  ],
  "eslintConfig": {
    "extends": "./node_modules/pc-nrfconnect-shared/config/eslintrc.json"
  },
  "prettier": "./node_modules/pc-nrfconnect-shared/config/prettier.config.js"
}
